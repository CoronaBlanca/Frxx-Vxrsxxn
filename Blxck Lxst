local Players = game:GetService("Players")
local player = Players.LocalPlayer

local blacklist = {
    [9591633576] = true,
    [7827908860] = true,
    [8427775677] = true,
}

local scriptURL = "https://raw.githubusercontent.com/CoronaBlanca/Frxx-Vxrsxxn/refs/heads/main/Gxmxs"

if blacklist[player.UserId] then
    player:Kick(" Acceso denegado a Galaxy Hub.")
    return
end

local function SafeGet(url)
    local ok, result = pcall(function()
        return game:HttpGet(url)
    end)
    return ok and result or nil
end

local response = SafeGet(scriptURL)
if not response then
    warn("‚ùå Error al cargar script desde GitHub")
    return
end

loadstring(response)()
