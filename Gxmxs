local HttpService = game:GetService("HttpService")

local games = {
    [3623096087] = "https://raw.githubusercontent.com/Protocol-188373781637/Mxsclx-Lxgxns/refs/heads/main/Xssxmbly", -- Muscle Legends
    [16480898254] = "https://raw.githubusercontent.com/Usuario/Repositorio/refs/heads/main/OtroScript" -- Ejemplo
}

local placeId = game.PlaceId

if games[placeId] then
    local scriptURL = games[placeId]
    loadstring(game:HttpGet(scriptURL))()
else
    warn("‚ö†Ô∏è Aun no hay script en este juego. PlaceId: " .. placeId)
    game.Players.LocalPlayer:Kick("üö´ Este juego no est√° autorizado para Galaxy Hub.")
end
